        <item name="baseTextColor">#fff</item>
        <item name="baseTextColorInverted">#000</item>
        <item name="baseSecondaryTextColor">#c7c7c7</item>
        <item name="baseSecondaryTextColorInverted">#2d2d2d</item>

        <item name="deleteIcon">@drawable/round_delete_white_24</item>
        <item name="editIcon">@drawable/round_create_white_24</item>
        <item name="addIcon">@drawable/round_add_circle_outline_white_18</item>
        <item name="cancelIcon">@drawable/outline_cancel_white_24</item>
        <item name="doneIcon">@drawable/round_check_circle_outline_white_24</item>
